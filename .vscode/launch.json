{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch and Debug Blazor Server",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/PatinaBlazor/PatinaBlazor/bin/Debug/net8.0/PatinaBlazor.dll",
      "args": [],
      "cwd": "${workspaceFolder}/PatinaBlazor/PatinaBlazor",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": "Launch and Debug Blazor WebAssembly in Chrome",
      "type": "blazorwasm",
      "request": "launch",
      "cwd": "${workspaceFolder}/PatinaBlazor/PatinaBlazor",
      "browser": "chrome"
    },
    {
      "name": "Debug Blazor WebAssembly in Chrome (HTTPS)",
      "type": "blazorwasm",
      "request": "attach",
      "cwd": "${workspaceFolder}",
      "url": "https://localhost:7047"
    },
    {
      "name": "Debug Blazor WebAssembly in Chrome (HTTP)",
      "type": "blazorwasm",
      "request": "attach",
      "cwd": "${workspaceFolder}",
      "url": "http://localhost:5265"
    }
  ]
}